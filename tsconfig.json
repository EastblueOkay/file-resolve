{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "es5",
    "module": "CommonJS",
    "outDir": "lib",
    "resolveJsonModule": true,
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "experimentalDecorators": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
